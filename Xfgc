const calculateDeviceTrends = (regionDevices) => {
    const trends = {};

    Object.keys(regionDevices).forEach((deviceType) => {
        trends[deviceType] = regionDevices[deviceType].map(device => {
            return {
                id: device.id,
                name: device.name,
                uptime: device.uptime || 0,
                downtime: device.downtime || 0,
                downtimeDuration: device.downtimeDuration || 0
            };
        });
    });

    return trends;
};


const { calculateDeviceTrends } = require('../utils');

